      "source": [
        "### Team APIs"
      ],
      "metadata": {
        "id": "Hlg1yfddcdD8"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "teams = client.teams.all()\n",
        "teams"
      ],
      "metadata": {
        "id": "M1Vy9hgbBY8T"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "link = 'https://statsapi.web.nhl.com/api/v1/teams'\n",
        "teams = pd.read_json(link)\n",
        "teams = pd.concat([teams.drop(['copyright','teams'], axis=1), teams['teams'].apply(pd.Series)], axis=1)\n",
        "teams.rename(columns={'id': 'teamId', 'link': 'teamLink', 'abbreviation': 'teamAbb'}, inplace=True)\n",
        "teams = pd.concat([teams.drop(['division','shortName','name'], axis=1), teams['division'].apply(pd.Series)], axis=1)\n",
        "teams.rename(columns={'id': 'divisionId','name': 'divisionName', 'link': 'divisionLink', 'nameShort': 'divisionAbb'}, inplace=True)\n",
        "teams = pd.concat([teams.drop(['venue','abbreviation'], axis=1), teams['venue'].apply(pd.Series)], axis=1)\n",
        "teams.rename(columns={'id': 'venueId','name': 'venueName', 'link': 'venueLink', 'city': 'venueCity'}, inplace=True)\n",
        "teams = pd.concat([teams.drop(['timeZone'], axis=1), teams['timeZone'].apply(pd.Series)], axis=1)\n",
        "teams.rename(columns={'id': 'timeZone','offset': 'timeDiff'}, inplace=True)\n",
        "teams = pd.concat([teams.drop(['franchise','teamName'], axis=1), teams['franchise'].apply(pd.Series)], axis=1)\n",
        "teams.rename(columns={'link': 'franchiseLink',}, inplace=True)\n",
        "teams = pd.concat([teams.drop(['conference'], axis=1), teams['conference'].apply(pd.Series)], axis=1)\n",
        "teams.rename(columns={'id': 'conferenceId','name': 'conferenceName', 'link': 'conferenceLink', 'nameShort': 'divisionAbb'}, inplace=True)"
      ],
      "metadata": {
        "id": "sPtO6QuojP_D"
      },
      "execution_count": 114,
      "outputs": []
    },
